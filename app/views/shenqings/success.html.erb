<div id="wrapper" class="clearfix">
  
  <div id="header">
    
    <div class="container">
      
      <h1 id="title">
        <%= link_to "Focus Business", root_path%>
      </h1>
      
    </div> <!-- /container -->
    
  </div> <!-- /header --> 
  
  
  <div id="nav" class="clearfix">
    
    <div class="container">
    
      <ul id="main-nav">
        <li class="active">
          <%= link_to "最新活动", dongs_path%>
        <li>
          <%= link_to "奖学金申请", fellowships_path%>
        </li>
        
        <li>
          <%= link_to "美国高中联盟", hschools_path%>
        </li>
        
        <li class="active">
          <%= link_to "家长联盟", new_meng_path%>
        </li>       
        
      </ul>
      
    </div> <!-- /container -->
    
  </div> <!-- /nav -->
  
  
  <div id="page-title">
    
    <h1>奖学金申请</h1>
  </div> <!-- /page-title -->
  <div id="content">
    
    <div class="container">
      
      <div class="row">
        
        <div class="grid-12">
          
          <div class="pricing-plans plans-4">
            
            <div class="plan-container">
              <p class="text-success">恭喜你提交成功！</p>
              <%= link_to "返回首页", root_path %>
            </div> <!-- /plan-container -->
              
          </div> <!-- /pricing-plans -->
          
        </div> <!-- /grid-12 -->
        
      </div> <!-- /row -->
      
    </div> <!-- /container -->
    
  </div> <!-- /content -->
  
  
  
  <div id="footer">
    <%= render "layouts/footer"%>    
  </div> <!-- /footer -->
  
  
  
  <div id="copyright">
    
    <div class="container">
      
      <div class="row">
        <h2>友情链接</h1>
        <% @lian.each do |l| %>
        <div class="grid-2">
          <p><%= link_to "友情链接", l.addr %></p>
        </div> <!-- /grid-4 -->
         <% end %>
      </div> <!-- /row -->
      
    </div> <!-- /container -->
      
  </div> <!-- /copyright -->
  
</div> <!-- /wrapper -->

