<div id="wrapper" class="clearfix">
  
  <div id="header">
    
    <div class="container">
      
      <h1 id="title">
        <%= link_to "Focus Business", root_path%>
      </h1>
      
    </div> <!-- /container -->
    
  </div> <!-- /header --> 
  
  
  <div id="nav" class="clearfix">
    
    <div class="container">
    
     <ul id="main-nav">
        <li >
          <%= link_to "最新活动", dongs_path%>
        <li>
          <%= link_to "奖学金申请", fellowships_path%>
        </li>
        
        <li class="active">
          <%= link_to "美国高中联盟", hschools_path%>
        </li>
        
        <li>
          <%= link_to "家长联盟", new_meng_path%>
        </li>       
        
      </ul>
      
    </div> <!-- /container -->
    
  </div> <!-- /nav -->
  
  
  <div id="page-title">
    
    <h1>学校列表</h1>
  </div> <!-- /page-title -->
  
  
  
  
  <div id="content">
    
    <div class="container">
      
      <div class="row">
        
        <div class="grid-12">
          
          <div class="pricing-plans plans-4">
            
            <div class="plan-container">
              <table class="table table-bordered">
                <thead>
                  <tr>
                    <th>学校排名</th>
                    <th>学校类型</th>
                    <th>地理位置</th>
                    <th>宗教背景</th>
                    <th>学校规模</th>
                    <th>TOEFL成绩</th>
                  </tr>
                </thead>
                <tbody>
                  <% @people.each do |p| %>
                    <tr>
                      <td><%= p.ranking %></td>
                      <td><%= p.lei %></td>
                      <td><%= p.place %></td>
                      <td><%= p.religion %></td>
                      <td><%= p.scale %></td>
                      <td><%= p.result %></td>
                    </tr>
                  <% end %>
                </tbody>
              </table>
              
            </div> <!-- /plan-container -->
              
          </div> <!-- /pricing-plans -->
          
        </div> <!-- /grid-12 -->
        
      </div> <!-- /row -->
      
    </div> <!-- /container -->
    
  </div> <!-- /content -->
  
  
  
  <div id="footer">
    <%= render "layouts/footer"%>    
  </div> <!-- /footer -->
  
  
  
  <div id="copyright">
    
    <div class="container">
      
      <div class="row">
      
        <div id="rights" class="grid-6">
        © 2013 Propel UI. All Rights Reserved.
        </div> <!-- /grid-6 -->
        
        <div id="totop" class="grid-2">
          <%= link_to "关于我们", abouts_path %>
        </div> <!-- /grid-6 -->
        <div id="totop" class="grid-2">
          <%= link_to "联系我们", contacts_path %>
        </div> <!-- /grid-6 -->
        <div id="totop" class="grid-2">
          <%= link_to "加入我们", joins_path %>
        </div> <!-- /grid-6 -->
        
      </div> <!-- /row -->
      
    </div> <!-- /container -->
      
  </div> <!-- /copyright -->
  
</div> <!-- /wrapper -->

